package csc241hw01;

public class Customer {
    String name;
    String address;
    int lotSize;
    double price;
    Employee emp;


    public Customer(String n, String a, int ls, double p) {
        name = n;
        address = a;
        lotSize = ls;
        price = p;
    }


    public void setEmployee(Employee e) {
        emp = e;
    }

    

    public void printEmployee() {
        System.out.println();
    }

    public String toString(){
        return "Name:" + name +" Address:" + address + " Lot Size:" + lotSize + " Price:" + price + " Employee:" + emp.name;
    }
}
